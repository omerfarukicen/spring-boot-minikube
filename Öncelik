 2004  wget https://storage.googleapis.com/minikube/releases/latest/minikube-linux-amd64
 2005  sudo cp minikube-linux-amd64 /usr/local/bin/minikube
 2006  sudo chmod 755 /usr/local/bin/minikube
 2007  minikube version
 2008  curl -LO https://storage.googleapis.com/kubernetes-release/release/`curl -s https://storage.googleapis.com/kubernetes-release/release/stable.txt`/bin/linux/amd64/kubectl
 2009  chmod +x ./kubectl
 2010  sudo mv ./kubectl /usr/local/bin/kubectl

